{
  "name": "quantum-profit-chain",
  "version": "0.1.0",
  "private": true,
  "description": "A futuristic crypto matrix game built with React TypeScript and Neural Glass design system",
  "author": "Quantum Profit Chain Team",
  "license": "MIT",
  "keywords": [
    "crypto",
    "blockchain",
    "matrix",
    "game",
    "web3",
    "ethereum",
    "defi",
    "neural-glass",
    "react",
    "typescript"
  ],
  "homepage": "https://quantum-profit-chain.app",
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@hookform/resolvers": "^5.2.1",
    "@metamask/detect-provider": "^2.0.0",
    "@tanstack/react-query": "^5.84.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/js-cookie": "^3.0.6",
    "@types/numeral": "^2.0.5",
    "@types/qrcode": "^1.5.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@types/web3": "^1.0.20",
    "@walletconnect/ethereum-provider": "^2.21.8",
    "@walletconnect/modal": "^2.7.0",
    "@walletconnect/web3-provider": "^1.8.0",
    "@web3-react/core": "^8.2.3",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "assert": "^2.1.0",
    "axios": "^1.11.0",
    "buffer": "^6.0.3",
    "copy-to-clipboard": "^3.3.3",
    "crypto-browserify": "^3.12.1",
    "date-fns": "^4.1.0",
    "ethers": "^6.15.0",
    "framer-motion": "10.18.0",
    "https-browserify": "^1.0.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.539.0",
    "numeral": "^2.0.6",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "qrcode": "^1.5.4",
    "react": "18.3.1",
    "react-app-rewired": "^2.2.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.8.0",
    "react-scripts": "5.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "styled-components": "^6.1.19",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "web-vitals": "^2.1.4",
    "web3": "^4.16.0",
    "yup": "^1.7.0"
  },
  "scripts": {
    "start": "craco start",
    "dev": "craco start",
    "build": "craco build",
    "build:analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --collectCoverageFrom='src/**/*.{ts,tsx}' --collectCoverageFrom='!src/**/*.d.ts'",
    "test:ci": "react-scripts test --coverage --ci --silent --maxWorkers=2",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "type-check": "tsc --noEmit",
    "clean": "rimraf build node_modules/.cache",
    "preview": "serve -s build -l 3000",
    "deploy": "npm run build && npm run preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "react-hooks/exhaustive-deps": "warn",
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "ajv": "8.17.1",
    "ajv-keywords": "5.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "sass": "^1.90.0",
    "serve": "^14.2.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.1"
  }
}
